client = discord.Client()

channelid = []

class MyClient(discord.Client):
    async def on_ready(self):
        print('Logged on as', self.user)
        
@client.event
async def on_member_join(member):
await message.channel.send(SE-LAM-LAAAR {member.mention})

client = MyClient()
